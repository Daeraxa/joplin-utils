{
  "name": "joplin-api",
  "version": "0.3.0",
  "description": "joplin ts/js data api",
  "license": "mit",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepublishOnly": "yarn build",
    "setup": "yarn build",
    "build": "rm dist && liuli-cli build pkg",
    "dev": "liuli-cli build pkg -w",
    "docs:dev": "yarn docs:build && live-server docs",
    "docs:build": "typedoc --out docs src && cp .nojekyll docs/",
    "docs:deploy": "yarn docs:build && gh-pages -d docs -e /joplin-api -a",
    "test": "jest --all",
    "ci": "yarn build"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "./test/jest-start.ts"
    ]
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "homepage": "https://joplin-utils.rxliuli.com/joplin-api",
  "repository": {
    "type": "git",
    "url": "https://github.com/rxliuli/joplin-utils/tree/master/libs/joplin-api"
  },
  "devDependencies": {
    "@liuli-util/cli": "^3.7.1",
    "@liuli-util/shell": "^0.2.0",
    "@types/form-data": "^2.5.0",
    "@types/fs-extra": "^9.0.4",
    "@types/jest": "^26.0.15",
    "@types/node-fetch": "^2.5.7",
    "fs-extra": "^9.0.1",
    "gh-pages": "^3.1.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.1",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "tslib": "^2.0.3",
    "typedoc": "^0.21.2",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@liuli-util/object": "^3.2.3",
    "axios": "^0.21.0",
    "form-data": "^3.0.0",
    "node-fetch": "^2.6.1",
    "query-string": "^6.13.7"
  }
}
