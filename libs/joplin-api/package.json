{
  "name": "joplin-api",
  "version": "0.3.0",
  "description": "joplin ts/js data api",
  "license": "mit",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "setup": "yarn build",
    "build": "liuli-cli clean && liuli-cli build pkg",
    "dev": "liuli-cli build pkg -w",
    "docs": "typedoc --out docs src && shx cp .nojekyll docs/",
    "predeploy": "yarn docs",
    "deploy": "gh-pages -d docs -e /joplin-api -a",
    "postdeploy": "shx rm -rf docs",
    "test": "jest --all",
    "ci": "yarn build"
  },
  "jest": {
    "preset": "ts-jest",
    "setupFilesAfterEnv": [
      "./test/jest-start.ts"
    ]
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "prettier": {
    "tabWidth": 2,
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "endOfLine": "crlf"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rxliuli/joplin-utils/tree/master/libs/joplin-api"
  },
  "devDependencies": {
    "@liuli-util/cli": "^3.4.0",
    "@types/form-data": "^2.5.0",
    "@types/fs-extra": "^9.0.4",
    "@types/jest": "^26.0.15",
    "@types/node-fetch": "^2.5.7",
    "fs-extra": "^9.0.1",
    "gh-pages": "^3.1.0",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.1",
    "prettier": "^2.1.2",
    "shx": "^0.3.3",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "tslib": "^2.0.3",
    "typedoc": "^0.20.36",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@liuli-util/object": "^3.2.2",
    "axios": "^0.21.0",
    "form-data": "^3.0.0",
    "node-fetch": "^2.6.1",
    "query-string": "^6.13.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts}": [
      "eslint --max-warnings=0",
      "prettier --ignore-path .eslintignore --write",
      "git add"
    ]
  }
}
